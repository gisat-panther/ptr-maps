{
	"name": "@gisatcz/ptr-maps",
	"version": "1.25.0",
	"description": "Panther FE maps",
	"sideEffects": false,
	"keywords": [
		"panther",
		"gisat"
	],
	"homepage": "https://github.com/gisat/ptr-maps",
	"prettier": "@gisatcz/prettier-config",
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	},
	"style": "dist/index.css",
	"main": "dist/index.js",
	"module": "lib/index.js",
	"files": [
		"dist",
		"lib",
		"scripts"
	],
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/gisat/ptr-maps"
	},
	"peerDependencies": {
		"react": "^16.13.1 || ^17.0.2 || ^18.3.1",
		"react-dom": "^16.13.1 || ^17.0.2 || ^18.3.1"
	},
	"dependencies": {
		"@deck.gl/core": "^9.0.36",
		"@deck.gl/extensions": "^9.0.36",
		"@deck.gl/geo-layers": "^9.0.36",
		"@deck.gl/layers": "^9.0.36",
		"@deck.gl/mesh-layers": "^9.0.36",
		"@deck.gl/react": "^9.0.36",
		"@gisatcz/cross-package-react-context": "^0.2.0",
		"@gisatcz/deckgl-geolib": "^1.11.2",
		"@gisatcz/ptr-atoms": "^1.16.0",
		"@gisatcz/ptr-core": "^1.8.1",
		"@gisatcz/ptr-utils": "^1.6.0",
		"@loaders.gl/core": "^4.1.4",
		"@luma.gl/constants": "^9.0.27",
		"@luma.gl/core": "^9.0.27",
		"@mapbox/sphericalmercator": "^1.2.0",
		"@math.gl/core": "^4.1.0",
		"chroma-js": "^2.4.2",
		"classnames": "^2.5.1",
		"find-up": "5.0.0",
		"lodash": "^4.17.21",
		"moize": "^6.1.6",
		"proj4": "^2.15.0",
		"prop-types": "^15.8.1",
		"react-is": "^18.3.1",
		"react-loadable": "^5.5.0",
		"react-resize-detector": "^11.0.1",
		"replace-in-file": "^6.3.5"
	},
	"devDependencies": {
		"@babel/cli": "^7.26.4",
		"@babel/core": "^7.26.0",
		"@babel/plugin-transform-runtime": "^7.25.9",
		"@babel/preset-env": "^7.26.0",
		"@babel/preset-react": "^7.26.3",
		"@babel/register": "^7.25.9",
		"@gisatcz/prettier-config": "0.0.3",
		"@rollup/plugin-babel": "^6.0.4",
		"@rollup/plugin-commonjs": "^28.0.1",
		"@rollup/plugin-image": "^3.0.3",
		"@rollup/plugin-multi-entry": "^6.0.1",
		"babel-plugin-lodash": "^3.3.4",
		"chai": "^4.3.7",
		"cross-env": "^7.0.3",
		"eslint": "^8.35.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.32.2",
		"husky": "^8.0.3",
		"mocha": "^10.2.0",
		"npm-run-all": "^4.1.5",
		"nyc": "^15.1.0",
		"postcss": "^8.4.49",
		"prettier": "^2.8.4",
		"pretty-quick": "^3.1.3",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"rimraf": "^4.1.2",
		"rollup": "^4.28.1",
		"rollup-plugin-filesize": "^10.0.0",
		"rollup-plugin-postcss": "^4.0.2",
		"run-s": "^0.0.0",
		"sass": "^1.58.3"
	},
	"scripts": {
		"clean": "rimraf dist",
		"start": "cross-env BABEL_ENV=development run-s clean && run-p start:es start:cjs start:lib:es copy:stylesheets copy:assets",
		"start:lib:es": "cross-env BABEL_ENV=development babel --watch src -d lib --verbose",
		"start:cjs": "cross-env BABEL_ENV=development NODE_ENV=cjs rollup -c -w",
		"start:es": "cross-env BABEL_ENV=development NODE_ENV=es rollup -c -w",
		"build": "cross-env BABEL_ENV=production run-s clean && run-p build:es build:cjs build:lib:es",
		"build:es": "cross-env BABEL_ENV=production NODE_ENV=es rollup -c",
		"build:cjs": "cross-env BABEL_ENV=production NODE_ENV=cjs rollup -c",
		"build:lib:es": "cross-env BABEL_ENV=production npm-run-all build:babel:lib:es copy:stylesheets copy:assets",
		"build:babel:lib:es": "cross-env BABEL_ENV=production babel src -d lib",
		"copy:stylesheets": "node ./build/copy-stylesheets.js",
		"copy:assets": "node ./build/copy-assets.js",
		"test": "cross-env NODE_ENV=test mocha",
		"test:watch": "cross-env NODE_ENV=test mocha --watch",
		"coverage": "cross-env NODE_ENV=test nyc --reporter=html --reporter=json npm run test",
		"format": "prettier --write .",
		"postinstall": "node scripts/postinstall.mjs",
		"lint": "eslint .",
		"lintFix": "eslint . --fix"
	}
}
