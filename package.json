		"@deck.gl/extensions": "^8.6.8",
		"@deck.gl/geo-layers": "^8.6.8",
		"@deck.gl/layers": "^8.6.8",
		"@deck.gl/mesh-layers": "^8.6.8",
		"@deck.gl/react": "^8.7.10",
		"@luma.gl/constants": "^8.5.17",
		"@gisatcz/cross-package-react-context": "^0.2.0",
		"@gisatcz/ptr-atoms": "^1.6.0",
		"@gisatcz/ptr-core": "^1.7.0",
		"@gisatcz/ptr-tile-grid": "^0.2.0",
		"@gisatcz/ptr-utils": "^1.6.0",
		"@loaders.gl/core": "^3.2.3",
		"@mapbox/sphericalmercator": "^1.2.0",
		"@react-leaflet/core": "^2.0.0",
		"@turf/boolean-point-in-polygon": "^6.5.0",
		"@turf/center-of-mass": "^6.5.0",
		"@turf/centroid": "^6.5.0",
		"@turf/flip": "^6.5.0",
		"@turf/helpers": "^6.5.0",
		"@turf/nearest-point": "^6.5.0",
		"chroma-js": "^2.1.2",
		"classnames": "^2.3.1",
		"find-up": "^5.0.0",
		"geojson-rbush": "^3.2.0",
		"georaster": "^1.5.6",
		"georaster-layer-for-leaflet": "^3.5.0",
		"js-quadtree": "^3.3.6",
		"leaflet": "^1.7.1",
		"lodash": "^4.17.21",
		"memoize-one": "^6.0.0",
		"path": "^0.12.7",
		"proj4": "^2.7.5",
		"proj4leaflet": "^1.0.2",
		"prop-types": "^15.7.2",
		"react-is": "^18.1.0",
		"react-leaflet": "^4.0.0",
		"react-loadable": "^5.5.0",
		"react-resize-detector": "^7.1.1",
		"replace-in-file": "^6.2.0",
		"shallow-equal": "^1.2.1",
		"uri-templates": "^0.2.0",
		"webworldwind-esa": "^1.0.3"
	},
	"devDependencies": {
		"@babel/cli": "^7.17.0",
		"@babel/core": "^7.17.0",
		"@babel/plugin-transform-runtime": "^7.17.0",
		"@babel/preset-env": "^7.16.11",
		"@babel/preset-react": "^7.16.7",
		"@babel/register": "^7.17.0",
		"@gisatcz/prettier-config": "0.0.3",
		"@rollup/plugin-alias": "^3.1.9",
		"@rollup/plugin-babel": "^5.3.0",
		"@rollup/plugin-commonjs": "^22.0.0",
		"@rollup/plugin-image": "^2.1.1",
		"@rollup/plugin-multi-entry": "^4.1.0",
		"babel-plugin-lodash": "^3.3.4",
		"chai": "^4.3.6",
		"cross-env": "^7.0.3",
		"eslint": "^8.14.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.29.4",
		"husky": "^5.0.9",
		"mocha": "^10.0.0",
		"npm-run-all": "^4.1.5",
		"nyc": "^15.1.0",
		"postcss": "^8.4.6",
		"prettier": "^2.5.1",
		"pretty-quick": "^3.1.1",
		"react": "^18.1.0",
		"react-dom": "^18.1.0",
		"rimraf": "^3.0.2",
		"rollup": "^2.67.0",
		"rollup-plugin-filesize": "^9.1.2",
		"rollup-plugin-postcss": "^4.0.2",
		"run-s": "^0.0.0",
		"sass": "^1.49.7"
	},
	"scripts": {
		"clean": "rimraf dist",
		"start": "cross-env BABEL_ENV=development run-s clean && run-p start:es start:cjs start:lib:es && copy:stylesheets copy:assets",
		"start:lib:es": "cross-env BABEL_ENV=development babel --watch src -d lib --verbose",
		"start:cjs": "cross-env BABEL_ENV=development NODE_ENV=cjs rollup -c -w",
		"start:es": "cross-env BABEL_ENV=development NODE_ENV=es rollup -c -w",
		"build": "cross-env BABEL_ENV=production run-s clean && run-p build:es build:cjs build:lib:es",
		"build:es": "cross-env BABEL_ENV=production NODE_ENV=es rollup -c",
		"build:cjs": "cross-env BABEL_ENV=production NODE_ENV=cjs rollup -c",
		"build:lib:es": "cross-env BABEL_ENV=production npm-run-all build:babel:lib:es copy:stylesheets copy:assets",
		"build:babel:lib:es": "cross-env BABEL_ENV=production babel src -d lib",
		"copy:stylesheets": "node ./build/copy-stylesheets.js",
		"copy:assets": "node ./build/copy-assets.js",
		"test": "cross-env NODE_ENV=test mocha",
		"test:watch": "cross-env NODE_ENV=test mocha --watch",
		"coverage": "cross-env NODE_ENV=test nyc --reporter=html --reporter=json npm run test",
		"format": "prettier --write .",
		"postinstall": "node scripts/postinstall.mjs",
		"lint": "eslint .",
		"lintFix": "eslint . --fix"
	}
}
