{
	"name": "@gisatcz/ptr-maps",
	"version": "1.17.0",
	"description": "Panther FE maps",
	"sideEffects": false,
	"keywords": [
		"panther",
		"gisat"
	],
	"homepage": "https://github.com/gisat/ptr-maps",
	"prettier": "@gisatcz/prettier-config",
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	},
	"style": "dist/index.css",
	"main": "dist/index.js",
	"module": "lib/index.js",
	"files": [
		"dist",
		"lib",
		"scripts"
	],
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/gisat/ptr-maps"
	},
	"peerDependencies": {
