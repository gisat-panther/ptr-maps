{
	"name": "@gisatcz/ptr-maps",
	"version": "1.8.0",
	"description": "Panther FE maps",
	"keywords": [
		"panther",
		"gisat"
	],
	"homepage": "https://github.com/gisat/ptr-maps",
	"prettier": "@gisatcz/prettier-config",
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	},
	"style": "dist/style.css",
	"main": "dist/index.js",
	"module": "dist/index.es.js",
	"files": [
		"dist",
		"lib",
		"scripts"
	],
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/gisat/ptr-maps"
	},
	"dependencies": {
		"@deck.gl/core": "^8.5.3",
		"@deck.gl/extensions": "^8.5.3",
		"@deck.gl/geo-layers": "^8.5.3",
		"@deck.gl/layers": "^8.5.3",
		"@deck.gl/mesh-layers": "^8.5.3",
		"@deck.gl/react": "^8.5.3",
		"@gisatcz/cross-package-react-context": "^0.0.2",
		"@gisatcz/ptr-atoms": "^1.3.12",
		"@gisatcz/ptr-core": "^1.4.0",
		"@gisatcz/ptr-tile-grid": "^0.0.9",
		"@gisatcz/ptr-utils": "^1.4.0",
		"@turf/boolean-point-in-polygon": "^6.5.0",
		"@turf/center-of-mass": "^6.5.0",
		"@turf/centroid": "^6.5.0",
		"@turf/flip": "^6.5.0",
		"@turf/helpers": "^6.5.0",
		"@turf/nearest-point": "^6.5.0",
		"auto": "^10.29.3",
		"chroma-js": "^2.1.2",
		"classnames": "^2.3.1",
		"find-up": "^5.0.0",
		"geojson-rbush": "^3.2.0",
		"georaster": "^1.5.4",
		"georaster-layer-for-leaflet": "^3.2.0",
		"js-quadtree": "^3.3.6",
		"leaflet": "^1.7.1",
		"lodash": "^4.17.21",
		"memoize-one": "5.2.1",
		"path": "^0.12.7",
		"postcss": "^8.3.6",
		"proj4leaflet": "1.0.2",
		"prop-types": "15.7.2",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-leaflet": "2.7.0",
		"react-loadable": "^5.5.0",
		"react-resize-detector": "4.2.3",
		"replace-in-file": "^6.2.0",
		"shallow-equal": "1.2.1",
		"uri-templates": "0.2.0",
		"webworldwind-esa": "1.0.3"
	},
	"devDependencies": {
		"@babel/cli": "^7.14.8",
		"@babel/core": "^7.15.0",
		"@babel/preset-env": "^7.15.0",
		"@babel/preset-react": "^7.14.5",
		"@babel/register": "^7.15.3",
		"@gisatcz/prettier-config": "0.0.3",
		"@rollup/plugin-alias": "^3.1.5",
		"@rollup/plugin-babel": "^5.3.0",
		"@rollup/plugin-commonjs": "^20.0.0",
		"@rollup/plugin-image": "^2.1.1",
		"@rollup/plugin-multi-entry": "^4.1.0",
		"babel-plugin-lodash": "^3.3.4",
		"chai": "^4.3.4",
		"husky": "^5.0.9",
		"mocha": "^9.1.1",
		"npm-run-all": "^4.1.5",
		"nyc": "^15.1.0",
		"prettier": "^2.3.2",
		"pretty-quick": "^3.1.0",
		"rimraf": "^3.0.2",
		"rollup": "^2.56.3",
		"rollup-plugin-filesize": "^9.1.1",
		"rollup-plugin-image-files": "^1.4.2",
		"rollup-plugin-node-externals": "^2.2.0",
		"rollup-plugin-postcss": "^4.0.1",
		"rollup-plugin-sass": "^1.2.6"
	},
	"scripts": {
		"clean": "rimraf dist",
		"start": "run-s clean && run-p start:es start:cjs start:lib:es",
		"start:lib:es": "cross-env BABEL_ENV=es babel --watch src -d lib --verbose",
		"start:cjs": "cross-env NODE_ENV=cjs rollup -c -w",
		"start:es": "cross-env NODE_ENV=es rollup -c -w",
		"build": "run-s clean && run-p build:es build:cjs build:lib:es",
		"build:es": "cross-env NODE_ENV=es rollup -c",
		"build:cjs": "cross-env NODE_ENV=cjs rollup -c",
		"build:lib:es": "npm-run-all build:babel:lib:es copy:stylesheets copy:assets",
		"build:babel:lib:es": "cross-env BABEL_ENV=es babel src -d lib",
		"copy:stylesheets": "node ./build/copy-stylesheets.js",
		"copy:assets": "node ./build/copy-assets.js",
		"test": "mocha",
		"test:watch": "mocha --watch",
		"coverage": "cross-env NODE_ENV=test nyc --reporter=html --reporter=json npm run test",
		"format": "prettier --write .",
		"postinstall": "node scripts/postinstall.js"
	}
}
